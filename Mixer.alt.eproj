{"schemaVersion":2,"id":"m6YGx6rIQMQHTXB6VMFw","name":"Ableton Mixer v6 alt","description":"Ableton Live remote script mixer (with control/group id collisions fixed). This version has the transport controls on every page (sacrificing one return track and the rumble toggle on the channel eq)","lua":"function patch.onRequest (device)\n  print (\"Patch Request pressed\");\n  if device.id == 7\n    then midi.sendSysex(PORT_1, {0x00, 0x21, 0x45, 0x7E, 0x7E})\n  end\nend\n\nfunction aa()\n   window.stop()\nend\n\nfunction zz()\n   window.resume()\nend\n\nfunction utl(idx,label)\n   -- update track labels for track with index idx (starting at 0) with label\n   -- tracks page\n   local group = groups.get(idx+145)\n   group:setLabel(label)\n   -- channel eq page\n   local group = groups.get(idx+153)\n   group:setLabel(label)\n   -- sends page\n   local group = groups.get(idx+159)\n   group:setLabel(label)\nend\n\nfunction ursl(idx,label)\n   -- update return track label and associated controls for return track with index idx (starting at 0) with label\n   -- set label\n   local group = groups.get(idx+164)\n   group:setLabel(label)\n   -- set all send controls (also invisible ones)\n   for i=0,4 do\n      local control = controls.get(72+6*idx+i+1)\n      control:setName(label)\n   end\nend\n\nfunction seqv(idx,flag)\n   -- Set the visibility of the channel eq device on the specified track (0..4, 5=master)\n    -- - controls\n   for r=0,4 do\n\t   local control = controls.get(36+6*r+idx+1)\n      control:setVisible(flag)\n   end\nend\n\nfunction sav(idx,flag)\n   -- Set the visibility of the arm button on the specified track (0..4)\n   local control = controls.get(24+idx+1)\n   control:setVisible(flag)\nend\n\nfunction setvalue(idx,str)\n   -- set the value for control idx to str\n   local dial = controls.get(idx)\n   local value = dial:getValue(\"value\")\n   value:overrideValue(str)\nend\n\nfunction st(str)\n   -- set the value for the tempo dial \n   setvalue(36,str)\n   setvalue(84,str)\n   setvalue(107,str)\n   setvalue(130,str)\nend\n\nfunction sp(str)\n   -- set the value for the postion dial \n   setvalue(35,str)\n   setvalue(78,str)\n   setvalue(106,str)\n   setvalue(129,str)\nend\n\nfunction smv(tc,rc)\n   -- Set the visibility of the controls and group labels when tc tracks and rc returns are active\n   -- main page\n   -- - group labels\n   for i=1,tc do\n      local group = groups.get(i+144)\n\t   group:setVisible(true)\n   end\n   for i=tc+1,5 do\n      local group = groups.get(i+144)\n\t   group:setVisible(false)\n   end\n   -- - controls\n   for c=0,tc-1 do\n      for r=0,4 do\n\t      local control = controls.get(6*r+c+1)\n\t      control:setVisible(true)\n\t   end\n   end        \n   for c=tc,4 do\n      for r=0,4 do\n\t      local control = controls.get(6*r+c+1)\n\t      control:setVisible(false)\n\t   end\n   end        \n   -- channel eq page:\n   -- - group labels\n   for i=1,tc do\n      local group = groups.get(i+152)\n      group:setVisible(true)\n   end\n   for i=tc+1,5 do\n      local group = groups.get(i+152)\n      group:setVisible(false)\n   end\n   -- - controls\n   for c=0,tc-1 do\n      for r=0,4 do\n\t      local control = controls.get(36+6*r+c+1)\n         control:setVisible(true)\n      end\n   end        \n   for c=tc,4 do\n      for r=0,4 do\n\t      local control = controls.get(36+6*r+c+1)\n\t      control:setVisible(false)\n\t   end\n   end\n   -- sends page\n   -- - group labels\n   for i=1,tc do\n      local group = groups.get(i+158)\n      group:setVisible(true)\n   end\n   for i=tc+1,5 do\n      local group = groups.get(i+158)\n      group:setVisible(false)\n   end\n   -- - controls\n   for c=0,tc-1 do\n      for r=0,rc-1 do\n\t      local control = controls.get(72+6*r+c+1)\n         control:setVisible(true)\n      end\n   end        \n   for c=tc,4 do\n      for r=0,4 do\n         local control = controls.get(72+6*r+c+1)\n\t      control:setVisible(false)\n      end\n   end      \n   for c=0,4 do\n      for r=rc,4 do\n\t      local control = controls.get(72+6*r+c+1)\n         control:setVisible(false)\n\t   end\n   end        \n   -- returns page\n   -- - group labels\n   for i=1,rc do\n      local group = groups.get(i+163)\n\t   group:setVisible(true)\n   end\n   for i=rc+1,5 do\n      local group = groups.get(i+163)\n\t   group:setVisible(false)\n   end\n   -- - controls\n   for c=0,rc-1 do\n      for r=0,2 do\n\t      local control = controls.get(108+6*r+c+1)\n\t      control:setVisible(true)\n\t   end\n      local control = controls.get(130+c+1)\n\t   control:setVisible(true)\n   end        \n   for c=rc,4 do\n      for r=0,2 do\n\t      local control = controls.get(108+6*r+c+1)\n\t      control:setVisible(false)\n\t   end\n      local control = controls.get(130+c+1)\n\t   control:setVisible(false)\n   end        \nend\n\nfunction formatPan (valueObject, value)\n   if value < 0 then \n      return (string.format(\"%iL\", -value))\n   elseif value == 0 then \n      return(\"C\")\n   else \n      return (string.format(\"%iR\", value))\n   end\nend\n\nfunction formatVolume (valueObject, value)\n   local table = {-70, -54.925, -44.646, -35.941, -28.798, -23.202, -19.175, -16.497, -13.999, -11.497, -8.996, -6.497, -3.998, -1.496, 1.004, 3.504, 6.0}\n   if value==16383\n     then return(\"6 dB\")\n   end\n   if value == 0\n      then return(\"-inf dB\")\n   end\n   local idx = math.floor(value / 1024)+1\n   local alpha = (value % 1024) / 1024.0\n   local beta = 0.1\n   local show = 0\n   if idx < 8\n      then show = table[idx] + (-beta * (alpha - 0.5) * (alpha - 0.5) + alpha + 0.25 * beta) * (table[idx+1] - table[idx])\n      else show = table[idx] + alpha * (table[idx+1] - table[idx])\n   end\n   return(string.format(\"%.1f dB\", show))  \nend\n\nfunction formatSendVolume (valueObject, value)\n   local table = {-71, -60.925, -50.646, -41.941, -34.798, -29.202, -25.175, -22.497, -19.999, -17.497, -14.996, -12.497, -9.998, -7.496, -4.996, -2.496, 0}\n   if value==18383\n     then return(\"0 dB\")\n   end\n   if value == 0\n      then return(\"-inf dB\")\n   end\n   local idx = math.floor(value / 1024)+1\n   local alpha = (value % 1024) / 1024.0\n   local beta = 0.1\n   local show = 0\n   if idx < 8\n      then show = table[idx] + (-beta * (alpha - 0.5) * (alpha - 0.5) + alpha + 0.25 * beta) * (table[idx+1] - table[idx])\n      else show = table[idx] + alpha * (table[idx+1] - table[idx])\n   end\n   return(string.format(\"%.1f dB\", show))\nend\n\nfunction formatAsFloat (valueObject, value)\n    return(string.format(\"%.1f dB\", value/100 ))\nend\n\nfunction formatAsSmallFloat (valueObject, value)\n    return(string.format(\"%.1f dB\", value/10 ))\nend\n\nfunction formatMidFreq (valueObject, value)\n   local table = {120, 155, 201, 261, 337, 437, 566, 733, 949, 1230, 1590, 2060, 2670, 3450, 4470, 5790, 7500}\n   if value==16383\n     then return(\"7.5 kHz\")\n   end\n   local idx = math.floor(value / 1024)+1\n   local alpha = (value % 1024) / 1024.0\n   local beta = -0.1\n   local show = table[idx] + (-beta * (alpha - 0.5) * (alpha - 0.5) + alpha + 0.25 * beta) * (table[idx+1] - table[idx]) \n   if show < 1000\n       then return(string.format(\"%i Hz\", math.floor(show) ))\n       else return(string.format(\"%.2f kHz\", show/1000 ))\n    end\nend","devices":[{"id":7,"name":"Generic MIDI","port":1,"channel":7,"instrumentId":"generic-midi"},{"id":8,"name":"Generic MIDI","port":1,"channel":8,"instrumentId":"generic-midi"},{"id":9,"name":"Generic MIDI","instrumentId":"generic-midi","port":1,"channel":9}],"tiles":[{"id":"02d8c68d-7f4c-43bb-8b76-0c65ae6f4472","name":"TRACK X","color":"FFFFFF","bounds":[0,16,146,16],"slotId":0,"values":[{"message":{"parameterNumber":127}}],"type":"label","span":1,"reference":145,"vspan":0},{"id":"930c8cce-9d08-44fc-90b4-6808c5cd97ca","name":"TRACK X+1","color":"FFFFFF","bounds":[170,16,146,16],"slotId":1,"values":[{"message":{"parameterNumber":128}}],"type":"label","span":1,"reference":146,